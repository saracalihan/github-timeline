{
  "name": "github-timeline",
  "version": "1.0.1",
  "description": "Best GitHub timeline component",
  "main": "js/scripts.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "lint": "eslint .",
    "minify": "minify js/scripts.js > js/scripts.min.js & minify css/style.css > css/style.min.css",
    "build-hook": "cp -R hooks ./.git/ && chmod +x ./.git/hooks/pre-commit",
    "sass-watch": "sass --watch css/main.scss css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saracalihan/github-timeline.git"
  },
  "keywords": [
    "GitHub",
    "Timeline",
    "Embeded HTML",
    "Embeded Timeline",
    "HTML",
    "JavaScript",
    "DOM"
  ],
  "author": "Alihan SARAC <<saracalihan@gmail.com>>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/saracalihan/github-timeline/issues"
  },
  "homepage": "https://github.com/saracalihan/github-timeline#readme",
  "dependencies": {
    "minify": "^7.0.2",
    "sass": "^1.35.2"
  },
  "devDependencies": {
    "eslint": "^7.26.0"
  }
}
